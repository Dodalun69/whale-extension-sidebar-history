{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": false,
    "allowUnreachableCode": true,
    "isolatedModules": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules/"
  ],
  "typeRoots": [
    "./node_modules/@types"
  ]
}
