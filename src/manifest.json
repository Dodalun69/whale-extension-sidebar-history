{
  "manifest_version": 2,
  "name": "whale-extension-boilerplate",
  "description": "whale-extension-boilerplate",

  "default_locale": "en",

  "browser_action": {
    "default_icon": {
      "16": "icon/icon-16.png",
      "48": "icon/icon-48.png",
      "128": "icon/icon-128.png"
    },
    "default_popup": "popupPage.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/mate131909/whale-extension-boilerplate/*"
      ],
      "css": ["contentScript-css-boilerplatePage.css"],
      "js": ["contentScript-script-boilerplatePage.bundle.js"]
    }
  ],
  "options_page": "optionPage.html",

  "permissions": [],
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
